+static int sci_ioctl(struct inode *inode, struct file *filp, unsigned long cmd, unsigned long arg)
+{
+	void __user *ptr = (void __user *)arg;
+	int ret = 0;
+
+	switch(cmd){
+//		case	IOCTL_GET_INIT_STATE :
+		case	1 :
