+#define EC_SCI_EVENT_BASE 0x21
+ec_handler ec_irq_handler[SCI_MAX_EVENT_COUNT];
+
+void ec_handler_install(int event_nu, ec_handler irq_handler)
+{
+    ec_irq_handler[event_nu - EC_SCI_EVENT_BASE] = irq_handler;
+}
