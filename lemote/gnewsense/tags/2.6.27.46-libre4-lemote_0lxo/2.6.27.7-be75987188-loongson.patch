+	if (ps2_command(ps2dev, ad->resp, v) != -1) {
+		ad->resp_cnt = (v >> 8) & (FSP_RESP_PKT_MAXLEN - 1);
+	} else {
+		ad->resp_cnt = 0;
+	}
+	return (count);
