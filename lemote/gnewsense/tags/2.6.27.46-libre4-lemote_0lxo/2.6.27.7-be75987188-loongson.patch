+/*******************************************************************/
+
+static void sci_display_lcd(void)
+{
+	unsigned char value;
+
+	outb(0x21, 0x3c4);
+	value = inb(0x3c5);
+	value |= (1 << 7);
+	outb(0x21, 0x3c4);
+	outb(value, 0x3c5);
+
+	outb(0x31, 0x3c4);
+	value = inb(0x3c5);
+	value = (value & 0xf8) | 0x01;
+	outb(0x31, 0x3c4);
+	outb(value, 0x3c5);
+
+	return;
+}
