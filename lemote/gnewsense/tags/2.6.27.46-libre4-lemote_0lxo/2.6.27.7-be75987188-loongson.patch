+*
+ * Misc. functions
+ *
+ **********************************************************************/
+// Load 8x8 pattern into local video memory
+void deLoadPattern(unsigned char* pattern, unsigned long write_addr)
+{
+    int i;
+    for (i = 0; i < (8 * 8 * 2); i += 4)
+    {
+//        memWrite32(write_addr, *(unsigned long*)(&pattern[i]));     removed by boyod
+        write_addr += 4;
+    }
+}
