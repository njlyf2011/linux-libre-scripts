      sed -n '
      : top
      $!{ N; b top; }
      ${
        : retry
        s,\([\n][ 	]*/[^\n]*\)/b notblob;[\n][ 	]*/\([^\n]*/b notblob;\),\1\\\\|\2,g
        t retry
        p;
	q;
      }'`
    ;;
  esac
}  

check () {
  case "$#" in 1) ;; *) echo ICE >&2; exit 1;; esac

  set_except "$1"

  set_sed_cmd "$1"

  case $1 in
  -) in= ;;
  *) in='< "$1"' ;;
  esac

  case $1 in
  *.bz2) cmd1='bunzip2' ;;
  *.gz) cmd1='gunzip' ;;
  *) cmd1='cat' ;;
  esac

  case $1 in
  *.tar*)
    cmd2="tar -xf - --to-command='echo \";;;begin \$TAR_FILENAME;;;\"; cat; echo \";;;end \$TAR_FILENAME;;;\"'";;
  *patch* | *diff*)
    cmd2='sed "/^[-]/d; s,^[ +],,"' ;;
  *)
    cmd2='cat' ;;
  esac

  cmd3='sed -n "$sedmain"'

  pipe="$cmd1 | $cmd2 | $cmd3"
  while :; do
    case "| $pipe |" in
    *"| cat |"*)
      pipe=`echo "| $pipe |" | sed 's,| cat |,|,g;s,^| ,,;s, |$,,;s,^|$,cat,'`
      ;;
    *)
      break ;;
    esac
  done

  eval $in $pipe
}

sepx="\\([,:{} 	LlUu\"\'\\\\\\n]\\+[xX\$]\\?\\|[ 	\\n]*[.][a-zA-Z][a-zA-Z0-9]*[ 	]\\+[\$]\\?\\)"
constx="[0-9][0-9a-fA-FxX]*"
unfcomment="/[/][*]\\([^*]\\|[*][*]*[^*/]\\|[*]*[\\n]\\)*[*]*\$/"

set_sedmain () {
  skiplonglines=
  case $1 in -f) skiplonglines="b noblobline;" ; shift; ;; esac

  sedmain="
$spex
/[\\n]/b recheck;
/^[ 	]*[/][*/]/{
  : collect_comment
  \$! { $unfcomment {
    N;
    b collect_comment;
  } }
  b recheck;
}
/^[ 	]*#[ 	]*define[ 	]/ {
  /\\\\[ 	]*\$/! b recheck;
  : collect_define
  \$! { $unfcomment {
    N;
    b collect_define;
  } }
  h;
  \$ b collect_end;
  n;
  : collect_define_loop
  \$! { $unfcomment {
    N;
    b collect_define_loop;
  } }
  H;
  \$ b collect_end
  /\\\\[ 	]*\$/ {
    n;
    b collect_define_loop
  }
  b collect_end;
}
/[=:][^;]*\$\\|[0-9{][,:\\\\0-9a-fA-FxX{}\"\' 	]*\$/ {
  : collect_begin
  \$! { $unfcomment {
    N;
    b collect_begin;
  } }
  h;
  \$ b collect_end;
  n;
  : collect_loop
  \$! { $unfcomment {
    N;
    b collect_loop;
  } }
  H;
  \$ b collect_end;
  /^#\\|[/][/*]\\|^[ 	]*\$\\|[{},=:\\\\0-9][{},:\\\\0-9a-fA-FxX\"\' 	]*\$/ {
    n;
    b collect_loop;
  }
  : collect_end
  g;
}
: recheck
$sedx
/^[^\\n]*$constx\\($sepx$constx\\)\\{$sens\\}/{
  b blob;
}
$skiplonglines
# These help us print longer matches.  Not useful if not printing blobs.
/^[^:\\n]*:[^\\n]*[\\n][^\\n]*$constx\\($sepx$constx\\)\\{$sens\\}\
\\|^[ 	]*#[ 	]*define[ 	]*\\([^\n]*\\|\\\\[ 	]*[\\n]\\|[/][*]\\([^*]\\|[*]\\+[^/]\\|[*]*[\\n]\\)*[*][/]\\)*$constx\\($sepx$constx\\)\\{$sens\\}\
\\|^[^\\n]*=\\([^{;\\n]*\\|[/][*]\\([^*]\\|[*]\\+[^/]\\|[*]*[\\n]\\)*[*][/]\\)*[\\n]\\?\\([^{;\\n]*\\|[/][*]\\([^*]\\|[*]\\+[^/]\\|[*]*[\\n]\\)*[*][/]\\)*{\\([^;\\n]*\\|[\\n]\\|[/][*]\\([^*]\\|[*]\\+[^/]\\|[*]*[\\n]\\)*[*][/]\\)*$constx\\($sepx$constx\\)\\{$sens\\}/b blob;
b noblobline;
: noblob
$5
b notblob
: unblobbed
$4
b recheck
: notblob
$3
d;
: noblobline
$2
D;
: blob
$1
d;
: unblob
s/{[{ 	\n]*$constx\\($sepx$constx\\)\\{$sens,\\}}[ 	]*;/{\/*(DEBLOBBED)*\/};/g
t unblobbed_some
s/$constx\\($sepx$constx\\)\\{$sens,\\}/\/*(DEBLOBBED)*\//g
t unblobbed
b noblobline
: unblobbed_some
s/$constx\\($sepx$constx\\)\\{$sens,\\}/\/*(DEBLOBBED)*\//g
t unblobbed
b noblobline
"
}

case $1 in
--mark-false-positives | -f)
  shift;
  set_sed_cmd () {
    set_sedmain -f "p;" "P;" "b unblob;" "" "p; d;"
  }
  ;;

--print-marked-false-positives | -P)
  shift;
  set_sed_cmd () {
    set_sedmain -f "" "" "b unblob;" "i\\
::: $file :::
p; d;" "d;"
  }
  ;;

--print-false-positives | -p)
  shift;
  set_sed_cmd () {
    set_sedmain -f "" "" "i\\
::: $file :::
p;" "d;" "d;"
  }
  ;;

--mark-blobs | -u)
  shift;
  set_sed_cmd () {
    set_sedmain -f "b unblob;" "P;" "p; d;"
  }
  ;;

--print-marked-blobs | -B)
  shift;
  set_sed_cmd () {
    set_sedmain "b unblob;" "" "d;" "i\\
::: $file :::
  p; d;"
  }
  ;;

--print-blobs | -b)
  shift;
  set_sed_cmd () {
    set_sedmain -f "i\\
::: $file :::
  p;"
  }
  ;;
*)
  set_sed_cmd () {
    set_sedmain -f "i\\
$file
  q;"
  }
  ;;
esac

case $# in
0)
  set fnord -
  shift
  ;;
esac

n=$#
for file
do
  if check "$file" | grep '.*'; then
      set fnord "$@" "$file"
      shift
  fi
done
shift $n

exec test $# = 0
