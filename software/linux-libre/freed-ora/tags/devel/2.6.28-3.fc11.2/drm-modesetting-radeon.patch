+
+	memset(&args, 0, sizeof(args));
+	args.sXTmdsEncoder.ucEnable = 1;
+
+	if (mode->clock > 165000)
+		args.sXTmdsEncoder.ucMisc = 1;
+	else
+		args.sXTmdsEncoder.ucMisc = 0;
+
+	// TODO 6-bit DAC
