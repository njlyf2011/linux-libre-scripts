+
+	memset(&args, 0, sizeof(args));
+	args.ucAction = 1;
+	if (mode->clock > 165000)
+		args.ucMisc = 1;
+	else
+		args.ucMisc = 0;
+
+	args.usPixelClock = cpu_to_le16(mode->clock / 10);
+
+	atom_execute_table(dev_priv->mode_info.atom_context, index, (uint32_t *)&args);
+}
